/*
//원의 면적 계산 프로그램
#include <stdio.h>

int main()
{
   double radius;
   double area;        //float보다 double이 비트수가 더 큼-애초에 큰걸쓰자 캠페인

   printf("반지름을 입력하시오: ");
   scanf("%lf", &radius);

   area = radius*radius*3.14;
   printf("원의 면적: %.2lf", area); //소수점 뒷자리
   return 0;
}

//자료형 항상 신경쓰기ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ
