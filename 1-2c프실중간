/*
// 형변환 문제! 다시 하기..ㅠㅠㅠㅠ 어려웡
#include <stdio.h>

int main()
{
    int i;
    double f;

    f = 5/4;
    printf("%f\n", f); //1.000000

    f = (double)5/4;    //실수형으로 변환되고 5.000/4.000
    printf("%f\n", f); // 1.250000

    f = 5.0/4;          //(=5.0/4.0) 실수로 계산됨
    printf("%f\n", f); // 1.250000

    f = 5.0/4.0;
    printf("%f\n", f); //1.250000

    f = (double)5 / (double)4;
    printf("%f\n", f);  //1.250000

    i = 1.3 + 1.8;
    printf("%d\n", i);  //실수끼리 계산되고 정수형으로 잘림

    i = (int)1.3 + (int)1.8;
    printf("%d", i);    //2

    return 0;
}*/
