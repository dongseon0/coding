/*
#include <stdio.h>

int main()
{
    char ch;
    printf("문자를 입력하시오:");
    scanf("%c", &ch);

    if (ch>='A'&& ch<='Z') //(==ch>=65&&ch<=90)
        printf("%c는 대문자입니다.", ch);
    else if(ch>='a'&& ch<='z')
        printf("%c는 소문자입니다.", ch);
    else if(ch>='0' && ch<='9')
        printf("%c는 숫자입니다.", ch);
    else
        printf("%c는 기타문자입니다.", ch);

    return 0;
}*/
/*
//산술 계산기
#include <stdio.h>

int main()
{
    int x, y, result;
    char op;

    printf("수식을 입력하시오:");
    scanf("%d %c %d", &x, &op, &y);
    printf(">>> %d %c %d\n", x, op, y);

    if(op=='+')
        result = x+y;
    else if(op=='-')
        result = x-y;
    else if(op=='/')
        result = x/y;
    else if(op=='*')
        result = x*y;
    else if(op=='%')
        result = x%y;
    else
        printf("지원하지 않는 연산자입니다.");

    printf("%d %c %d = %d", x, op, y, result);
    return 0;
}
*/
/*
#include <stdio.h>
#include <math.h>

int main()
{
    double a, b, c, dis, sol;  //ax*x+b*x+c =0

    printf("계수를 입력하시오:");
    scanf("%lf %lf %lf", &a, &b, &c);

    dis = b * b - (4 * a * c);

    if (dis < 0)
        printf("실근이 존재하지 않는다.");

    else if (dis >= 0)
    {
        if (a == 0)
        {
            sol = -c / b;
            printf("근은 %f이다.", sol);
        }

        else
        {
            sol = ((-b) + sqrt(dis)) / (2 * a);
            printf("근은 %f이다.", sol);

            sol = ((-b) - sqrt(dis)) / (2 * a);
            printf("근은 %f이다.", sol);
        }
    }


    return 0;
}
*/
/*
//유클리드 알고리즘으로 최대공약수 찾기
#include <stdio.h>

int main()
{
    int a, b, max, min, r;
    printf("두 정수를 입력하시오:");
    scanf("%d %d", &a, &b);
    max = (a>b)? a:b;
    min = (a>b)? b:a;

    while(min!=0)
    {
        r = max % min;
        max = min;
        min = r;
    }
    printf("공약수는 %d입니다.", max);

    return 0;
}*/
/*
//사용자가 0을 입력할 때까지 숫자를 더한다.
#include <stdio.h>

int main()
{
    int n, sum =0;      //sum 쓸거면 초기화!!!!!!!!!!!!!!!!!!!!!!!!!!

    do
    {
        printf("정수를 입력하시오:");
        scanf("%d", &n);
        sum+=n;

    } while (n!=0);

    printf("숫자들의 합 = %d", sum);

    return 0;
}*/
