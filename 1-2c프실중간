/*
//%는 나머지연산자
#include <stdio.h>
#define SEC_PER_MINUTE 60

int main()
{
    int input, minute, second;

    printf("초를 입력하시오: ");
    scanf("%d",&input);

    minute = input / SEC_PER_MINUTE;
    second = input % SEC_PER_MINUTE;
    printf("%d초는 %d분 %d초입니다.", input, minute, second);
    return 0;

