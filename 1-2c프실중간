/*
////////분모는 0이 될 수 없다!!
#include <stdio.h>

int main()
{
    int n1, n2;

    printf("분자를 입력하시오:");
    scanf("%d", &n1);

    printf("분모를 입력하시오:");
    scanf("%d", &n2);

    if(n2==0)
        printf("0으로 나눌 수 없습니다.");
    else
        printf("결과는 %d입니다.", n1/n2);

    return 0;
