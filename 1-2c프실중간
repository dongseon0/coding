/*
//연산자들
#include <stdio.h>

int main()
{
    int x = 0, y = 0;
    int z =10;
    int result;

    result = 2>3 || 6>7;
    printf("%d\n", result);

    result = 2 || 3 && 3>2; //??????????????????
    //거짓이 0, 나머진 다 참
    printf("%d\n", result);

    result = x = y = 2;
    printf("%d\n", result);

    //***중요: 위에서 2가 대입됨
    result = -++x+y--;  //-3 + 2 --> 위에서 2대입
    printf("%d\n", result);
    printf("%d\n", y); // y==1
    return 0;
